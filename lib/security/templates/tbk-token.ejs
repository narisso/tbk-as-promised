<wsse:SecurityTokenReference>
    <X509Data
        xmlns:ds="http://www.w3.org/2000/09/xmldsig#">
        <X509IssuerSerial>
            <X509IssuerName><%-iss%></X509IssuerName>
            <X509SerialNumber><%-serial%></X509SerialNumber>
        </X509IssuerSerial>
        <X509Certificate><%-binaryToken%></X509Certificate>
    </X509Data>
</wsse:SecurityTokenReference>